<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Proyecto WEB SENA</title>
    
     <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome Icons -->
    
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css">
    
    <!-- Theme style -->
    <link rel="stylesheet" href="~/adminlte/dist/css/adminlte.min.css">
    <!-- Datatables -->
    <link href="~/datatables/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <!-- AlertifyJS -->
    
    <link rel="stylesheet" href="~//lib/AlertifyJS/css/themes/bootstrap.css">
    
    <link rel="stylesheet" href="~/lib/AlertifyJS/css/alertify.css">
    <link href="~/css/Admin.css" rel="stylesheet" />
     
    @RenderSection("Styles", required: false)
</head>

<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <partial name="_Navbar" />
        <partial name="_Sidebar" />
        <div class="content-wrapper">
            <main role="main" class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </main>

        </div>
       
        <partial name="_Footer" />
           
       
    </div>

    <!--Modal global-->
    <div class="modal" tabindex="-1" role="dialog" id="form-modal">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title"></h5>
            <button type="button" class="close " data-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            
          </div>
          
        </div>
      </div>
    </div>



    <!-- REQUIRED SCRIPTS -->

    <!-- jQuery -->
    <script src="~/adminlte/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/adminlte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/adminlte/dist/js/adminlte.min.js"></script>
    <!-- Datatable -->
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    @*<script src="~/js/site.js" asp-append-version="true"></script>*@
    <script src="~/lib/limonte-sweetalert2/sweetalert2.min.js"></script>
    <!-- AlertifyJS -->
    <script src="~/lib/AlertifyJS/alertify.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
    
</body>
</html>